package com.ustglobal.jpawithhibernateapp.onetoone;

import javax.persistence.Entity;

@Entity
public class Person {

	private int pid;
	private String name;
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
